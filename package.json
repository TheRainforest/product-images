  "main": "index.js",
  "scripts": {
    "build": "webpack --config prod.config.js",
    "build:dev": "webpack --config prod.config.js -w",
    "start": "node server",
    "start:dev": "nodemon server",
    "csv": "node --max-old-space-size=8192 server/database/generateCSV",
    "test": "jest server/test/ --detectOpenHandles",
    "load-pg": "psql -d productimages -U ericsong -f server/database/postgres/schema.sql",
    "load-cs": "node server/database/cassandra",
    "seed-pg": "node server/database/postgres/importCSV",
    "seed-cs": "cqlsh -k productimages -e \"COPY productimages.items (id, images) from 'server/database/images.csv' WITH DELIMITER = '|' AND HEADER = TRUE;\"",
    "stress": "artillery run /Users/ericsong/Documents/HackReactor/SDC/product-images/server/stressTest/test.yml"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bluebird": "^3.7.2",
