    "express": "^4.17.1",
    "faker": "^4.1.0",
    "mongoose": "^5.9.10",
    "newrelic": "^6.8.0",
    "nodemon": "^2.0.4",
    "pg": "^8.2.1",
    "pg-promise": "^10.5.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "style-loader": "^1.2.1",
    "supertest": "^4.0.2",
    "webpack-cli": "^3.3.11"
  },
